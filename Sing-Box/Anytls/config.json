{
    "inbounds": [
        {
            "type": "anytls",
            "listen": "::",
            "listen_port": 10006,
            "users": [
                {
                    "password": "PASSWORD"
                }
            ],
            "tls": {
                "enabled": true,
                "server_name": "SERVER_NAME",
                "reality": {
                    "enabled": true,
                    "handshake": {
                        "server": "SERVER",
                        "server_port": 443
                    },
                    "private_key": "PRIVATE_KEY",
                    "short_id": [
                        "SHORT_ID"
                    ]
                }
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct"
        }
    ]
}
